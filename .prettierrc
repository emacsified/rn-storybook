{
  "useTabs": false,
  "printWidth": 100,
  "tabWidth": 2,
  "singleQuote": false,
  "trailingComma": "es5",
  "jsxBracketSameLine": false,
  "parser": "flow",
  "semi": true,
  "jsxSingleQuote": false,
  "bracketSpacing": true,
  "htmlWhitespaceSensitivity": "css",
  "endOfLine": "lf",
  "arrowParens": "always",
  "overrides": [
    {
        "files": "*.css",
        "options": {
            "parser": "css"
        }
    },
    {
        "files": "*.json",
        "options": {
            "parser": "json"
        }
    }
    ]
}
